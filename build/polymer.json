{
  "entrypoint": "index.html",
  "shell": "src/gdg-app.html",
  "fragments": [
    "src/gdg-home.html",
    "src/gdg-about.html",
    "src/gdg-events.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json",
    "index.html",
    "src/gdg-app.html",
    "src/gdg-home.html",
    "src/gdg-about.html",
    "src/gdg-events.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es5-bundled",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es5-bundled",
      "basePath": true
    },
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-unbundled",
      "basePath": true
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "minify": true,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true,
      "addPushManifest": true,
      "preset": "es6-bundled",
      "basePath": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}